ATOM                :=          ([A-Za-z][A-Za-z0-9]*)
STRING              :=          ("([^"])*")|('[^']*')
DIGITAL             :=          ([0-9]*\.[0-9]+)|([0-9]+)
OPERATOR            :=          (\+|-|\*|\\|<|>|==|<=|>=|!=)
VOPERATOR           :=          (=)
SVOPERATOR          :=          (\\+\\+|--)
NODE                ::          ATOM | STRING | DIGITAL
EXPRESSION          ::          NODE | NODE OPERATOR NODE
VARIABLE            ::          ATOM VOPERATOR EXPRESSION